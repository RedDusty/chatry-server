    "firebase-admin": "^10.0.2",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.2",
    "socket.io": "^4.4.1",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.12.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
